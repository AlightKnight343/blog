<%- include('partials/header') %> 
    <h1>Compose</h1>

    <form action="/compose" method="POST">
        <div class="form-group">
        <label for="postTitle">Title</label>
        <br> 
        <input class="form-control" type="text" name="postTitle">
        <br> <br>
        <label for="blog">Your post</label>
